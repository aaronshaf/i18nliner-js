{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "commonjs": true,
    "es2022": true,
    "node": false,
  },
  "rules": {
    "no-extra-semi": "off",
    "no-new-object": "error",
    "no-prototype-builtins": "off",
    "no-shadow": "error",
    "no-useless-call": "error",
    "no-useless-escape": "off",
  }
}
